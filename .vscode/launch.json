    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch RunTest",
            "request": "launch",
            "mainClass": "org.jjvm.test.RunTest",
            "projectName": "JJVM"
        },
        {
            "type": "java",
            "name": "Launch TestRuntime",
            "request": "launch",
            "mainClass": "org.jjvm.test.TestRuntime",
            "projectName": "JJVM"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch Main",
            "request": "launch",
            "mainClass": "org.jjvm.Main",
            "projectName": "JJVM",
            "args": [
                "-Xjre",
                "/usr/lib/jvm/java-8-openjdk-amd64/jre",
                "java.lang.String"
            ]
        },
        {
            "type": "java",
            "name": "Test Class File",
            "request": "launch",
            "mainClass": "org.jjvm.test.TestClassFile",
            "projectName": "JJVM",
            "args": [
                "-Xjre",
                "/usr/lib/jvm/java-8-openjdk-amd64/jre",
                "java.lang.String"
            ]
        }
    ]
}